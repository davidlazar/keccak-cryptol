// utility functions

md : {x} (fin x) => [8*x] -> [8*x];
md x = join(reverse(groupBy(8, x)));

r : {n x}
  ( fin n, fin x
  , n >= n%8
  , 8*((n+7)/8) >= n%8
  , n%8-8*((n+7)/8)+8*x >= 0
  , n%8-8*((n+7)/8)+8*x >= max(n%8-8*((n+7)/8),0)
  , 8*x-8*((n+7)/8) == 0
  , n%8-n+8*x >= max(n%8-n,0)
  ) => [8 * x] -> [n];
r X = h # t
  where {
    M = md X;
    h = take(`n - `n % 8, M);
    t = drop(8 * ((`n + 7) / 8) - `n % 8, M);
  };
